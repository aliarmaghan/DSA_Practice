class Solution {
    public int arraySign(int[] nums) {
        double mul=1;
        for(int i=0; i<nums.length; i++){
            mul = mul * nums[i];
        }
        if(mul < 0){
            return -1;
        } else if(mul > 0){
            return 1;
        }else{
            return 0;
        }
    }
}
